{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": true
  }
}
